{% extends 'base-initial.j2'%}
{% block interfaces%}
{% if type == 'switch' %}
interface Ethernet1/1
 no switchport
 ip vrf forwarding MGMT
 ip address {{ ip }} {{ mask }}
 no shutdown
{% elif type == 'router' %}
interface Ethernet0/2
 ip vrf forwarding MGMT
 ip address {{ ip }} {{ mask }}
 no shutdown
{% endif %}
{% endblock %}
